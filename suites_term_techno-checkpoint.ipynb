    "def somme_livret(N):\n",
    "    n=0\n",
    "    u=50\n",
    "    l=[]\n",
    "    for i in range(1,N+1):\n",
    "        l=l+[[n,u]]\n",
    "        n=n+1\n",
    "        u=u+40\n",
    "    return(l)"
